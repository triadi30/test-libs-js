{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        {
          "src": "/dropzone",
          "dest": "/index_dropzone.html"
        },
        {
          "src": "/filepond",
          "dest": "/index_filepond.html"
        }
      ]
  }
  